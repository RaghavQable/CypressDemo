{
  "dependencies": {
    "cypress-mochawesome-reporter": "^3.4.0",
    "frame": "^1.0.0-alpha.2",
    "iframe": "^1.0.0",
    "junit-report-merger": "^6.0.1",
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "cypress": "^12.12.0",
    "cypress-multi-reporters": "^1.6.3",
    "mocha-junit-reporter": "^2.2.0"
